    "eslint-config-next": "13.3.0",
    "next": "13.3.0",
    "pg": "^8.10.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/pg": "^8.6.6",
    "drizzle-kit": "^0.17.4",
